    },
    "0xC1Ebd02f738644983b6C4B2d440b8e77DdE276Bd": {
      "ignoreMethods": ["delayedInboxAccs", "sequencerInboxAccs"],
      "ignoreInWatchMode": [
        "delayedMessageCount",
        "sequencerMessageCount",
        "sequencerReportedSubMessageCount"
      ]
    },
    "0x3ffFbAdAF827559da092217e474760E2b2c3CeDd": {
      "fields": {
        "accessControl": {
          "handler": {
            "type": "accessControl"
          }
