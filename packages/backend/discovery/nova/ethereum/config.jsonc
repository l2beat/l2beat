    "L1Timelock": {
      "ignoreRelatives": ["governanceChainInbox", "l2Timelock"],
      "fields": {
        "accessControl": {
          "handler": {
            "type": "accessControl"
          }
        }
      }
    },
    // L1CustomGateway
    "0x23122da8C581AA7E0d07A36Ff1f16F799650232f": {
      "ignoreRelatives": ["counterpartGateway"]
    },
    "0xC840838Bc438d73C16c2f8b22D2Ce3669963cD48": {
