  "overrides": {
    "ValidatorUtils": {
      "fields": {
        "NOVA_STAKERS": {
          "handler": {
            "type": "call",
            "method": "function getStakers(address rollup, uint64 startIndex, uint64 max) view returns (address[], bool hasMore)",
