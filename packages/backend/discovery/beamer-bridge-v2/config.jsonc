      "ignoreInWatchMode": ["chains", "currentNonce"]
    },
    "EthereumFillManager": {
      "fields": {
        "liquidityProviders": {
          "type": "arrayFromTwoEvents",
          "addEvent": "LpAdded",
          "addKey": "lp",
          "removeEvent": "LpRemoved",
          "removeKey": "lp",
