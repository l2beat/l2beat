{
  "$schema": "../../../../../discovery/schemas/contract.v2.schema.json",
  "description": "Intermediary contract between the *Validators* and the ZKsync Era diamond that delays block execution (ie withdrawals and other L2 --> L1 messages) by {{executionDelay_fmt}}.",
  "ignoreMethods": ["committedBlockTimestamp", "getCommittedBatchTimestamp"],
  "fields": {
    "executionDelay_fmt": {
      "handler": {
        "type": "call",
        "method": "executionDelay",
        "args": []
      },
      "returnType": "(FormatSeconds)"
    },
    "zksyncValidatorsAdded": {
      "handler": {
        "type": "arrayFromOneEventWithArg",
        "event": "ValidatorAdded",
        "valueKey": "_addedValidator",
        "arg": "_chainId",
        "argValue": "324"
      }
    },
    "zksyncValidatorsRemoved": {
      "handler": {
        "type": "arrayFromOneEventWithArg",
        "event": "ValidatorRemoved",
        "valueKey": "_removedValidator",
        "arg": "_chainId",
        "argValue": "324"
      }
    },
    "cronosValidatorsAdded": {
