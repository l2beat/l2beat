    },
    "zeronetworkValidatorsRemoved": {
      "handler": {
        "type": "arrayFromOneEventWithArg",
        "event": "ValidatorRemoved",
        "valueKey": "_removedValidator",
        "arg": "_chainId",
        "argValue": "543210"
      }
    },
    "sophonValidatorsAdded": {
      "handler": {
        "type": "arrayFromOneEventWithArg",
        "event": "ValidatorAdded",
        "valueKey": "_addedValidator",
        "arg": "_chainId",
        "argValue": "50104"
      }
    },
    "sophonValidatorsRemoved": {
      "handler": {
        "type": "arrayFromOneEventWithArg",
