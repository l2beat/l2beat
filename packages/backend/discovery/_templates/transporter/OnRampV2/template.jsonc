{
  "$schema": "../../../../../discovery/schemas/contract.v2.schema.json",
  "ignoreMethods": ["getNops", "getAllowList", "getStaticConfig"],
  "ignoreRelatives": ["getSupportedTokens", "tokenAddresses", "linkToken"],
  "fields": {
    "tokenPools": {
      "handler": {
        "type": "arrayFromTwoEvents",
        "addEvent": "PoolAdded",
        "addKey": "pool",
        "removeEvent": "PoolRemoved",
        "removeKey": "pool"
      }
    },
    "linkToken": {
      "handler": {
        "type": "call",
        "args": [],
        "method": "getStaticConfig",
        "pickFields": ["linkToken"]
      }
    },
    "staticConfigAddresses": {
      "handler": {
        "type": "call",
        "args": [],
        "method": "getStaticConfig",
        "pickFields": ["prevOnRamp", "armProxy"]
      }
    },
    "staticConfigChains": {
      "handler": {
        "type": "call",
        "args": [],
        "method": "getStaticConfig",
        "pickFields": ["chainSelector", "destChainSelector"]
      }
    },
    "tokenAddresses": {
      "handler": {
        "type": "call",
        "method": "getSupportedTokens",
        "args": [],
        "ignoreRelative": true
      }
    }
  }
}
