    "ReplicaUpgradeBeacon": {
      "proxyType": "immutable"
    },
    "HomeUpgradeBeacon": {
      "proxyType": "immutable"
    },
    "GovernanceUpgradeBeacon": {
      "proxyType": "immutable"
    },
    "XAppConnectionManager": {
      "fields": {
        "watchers": {
          "type": "arrayFromOneEvent",
          "event": "WatcherPermissionSet",
          "valueKey": "watcher",
          "flagKey": "access"
        }
      }
    }
  }
