  "overrides": {
    "HomeBeaconProxy": {
      "proxyType": "Optics Beacon proxy"
    },
    "ReplicaBeaconProxy": {
      "proxyType": "Optics Beacon proxy",
      "ignoreInWatchMode": ["committedRoot"]
    },
    "BridgeRouterBeaconProxy": {
      "proxyType": "Optics Beacon proxy",
      "ignoreMethods": ["tokenBeacon"]
