    "0x33B0Ec794c15D6Cc705818E70d4CaCe7bCfB5Af3": "Boba_Adapter",
    "0xB130E3056D5C692300d66c12C10ffA2073d9424D": "Polygon_Adapter"
  },
  "overrides": {
    "HubPool": {
      "ignoreMethods": ["crossChainContracts", "getCurrentTime"],
      "fields": {
        "CrossChainContracts": {
          "handler": {
            "type": "stateFromEvent",
            "event": "CrossChainContractsSet",
            "returnParams": ["l2ChainId", "adapter", "spokePool"],
            "groupBy": "l2ChainId"
          }
        }
      },
      "ignoreRelatives": ["weth"],
      "ignoreInWatchMode": ["rootBundleProposal"]
    },
    "BondToken": {
      "fields": {
        "proposers": {
          "handler": {
            "type": "arrayFromOneEvent",
            "event": "ProposerModified",
            "valueKey": "proposer",
            "flagKey": "enabled"
          }
        }
      }
    },
    "0xB524735356985D2f267FA010D681f061DfF03715": {
      "ignoreInWatchMode": ["nonce"]
    },
    "EmergencyProposalExecutor": {
      "ignoreInWatchMode": ["nonce"]
    },
    "ProposerV2": {
      "ignoreMethods": ["bondedProposals"],
      "ignoreInWatchMode": ["bond", "getCurrentTime"]
    },
    "GovernorV2": {
      "ignoreInWatchMode": ["getCurrentTime", "numProposals"],
      "ignoreMethods": ["getProposal", "proposals"]
    },
    "EmergencyProposer": {
      "ignoreInWatchMode": ["getCurrentTime"]
    },
    "Ethereum_SpokePool": {
      "ignoreMethods": ["rootBundles"],
      "ignoreInWatchMode": ["numberOfDeposits", "getCurrentTime"],
      "fields": {
        "proxiableUUID": {
          "handler": {
            "type": "call",
            "method": "function proxiableUUID() view returns (bytes32)",
            "args": [],
            "expectRevert": true
          }
        }
      },
      "ignoreRelatives": ["wrappedNativeToken"]
    },
    "VotingToken": {
      "ignoreInWatchMode": ["totalSupply", "totalSupplyAt"]
    },
    "ZkSync_Adapter": {
      "ignoreInWatchMode": ["getL1CallValue"], // required amount of ETH to send a message, changes depending on the gas price
      "ignoreRelatives": [
        "zkErc20Bridge",
        "zkSyncEthBridge",
        "zkSyncMessageBridge",
        "l1Weth"
      ]
    },
    "Linea_Adapter": {
      "ignoreRelatives": [
        "L1_MESSAGE_SERVICE",
        "L1_TOKEN_BRIDGE",
        "L1_USDC_BRIDGE",
        "L1_WETH"
      ]
    },
    "Optimism_Adapter": {
      "ignoreRelatives": [
        "cctpTokenMessenger",
        "DAI",
        "DAI_OPTIMISM_BRIDGE",
        "L1_STANDARD_BRIDGE",
        "L1_WETH",
        "MESSENGER",
        "SNX",
        "SNX_OPTIMISM_BRIDGE",
        "usdcToken"
      ]
    },
    "Arbitrum_Adapter": {
      "ignoreRelatives": [
        "cctpTokenMessenger",
        "L1_DAI",
        "L1_ERC20_GATEWAY_ROUTER",
        "L1_INBOX",
        "L2_REFUND_L2_ADDRESS",
        "usdcToken"
      ]
    },
    "Base_Adapter": {
      "ignoreRelatives": [
        "cctpTokenMessenger",
        "L1_STANDARD_BRIDGE",
        "L1_WETH",
        "MESSENGER",
        "usdcToken"
      ]
    },
    "Boba_Adapter": {
      "ignoreRelatives": ["l1StandardBridge", "l1Weth", "messenger"]
    },
    "Polygon_Adapter": {
      "ignoreRelatives": [
        "cctpTokenMessenger",
        "DEPOSIT_MANAGER",
        "ERC20_PREDICATE",
        "FX_STATE_SENDER",
        "L1_MATIC",
        "L1_WETH",
        "ROOT_CHAIN_MANAGER",
        "usdcToken"
      ]
    },
    "OptimisticOracleV3": {
      "ignoreMethods": ["getCurrentTime"],
      "ignoreRelatives": ["cachedOracle", "defaultCurrency"]
    },
    "OptimisticGovernor": {
      "ignoreMethods": ["getCurrentTime"],
      "ignoreRelatives": ["collateral"]
    },
    "Arbitrum_Bridge": {
      "ignoreMethods": ["delayedInboxAccs", "sequencerInboxAccs"],
      "ignoreRelatives": ["allowedOutboxList", "rollup", "sequencerInbox"]
    },
    "Arbitrum_Old_Bridge": {
      "ignoreMethods": ["inboxAccs"]
    }
  }
}
