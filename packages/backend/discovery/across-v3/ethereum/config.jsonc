    "0xE233009838CB898b50e0012a6E783FC9FeE447FB": "ZkSync_Adapter",
    "0x7Ea0D1882D610095A45E512B0113f79cA98a8EfE": "Linea_Adapter",
    "0xb3a4e39F0CD9aBAc5d866f023C18e73224667Fee": "Optimism_Adapter",
    "0xd881A21F17B83AefFd11cc2e7363740449eb8069": "Arbitrum_Adapter",
    "0x8315177aB297bA92A06054cE80a67Ed4DBd7ed3a": "Arbitrum_Bridge",
    "0x011B6E24FfB0B5f5fCc564cf4183C5BBBc96D515": "Arbitrum_Old_Bridge",
