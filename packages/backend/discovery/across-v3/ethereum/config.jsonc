    "0xB524735356985D2f267FA010D681f061DfF03715": "HubPool Multisig",
    "0xee1DC6BCF1Ee967a350e9aC6CaaAA236109002ea": "BondToken",
    "0xfb55F43fB9F48F63f9269DB7Dde3BbBe1ebDC0dE": "OptimisticOracleV3",
