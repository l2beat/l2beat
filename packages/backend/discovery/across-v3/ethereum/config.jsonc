    "0x04Fa0d235C4abf4BcF4787aF4CF447DE572eF828": "VotingToken",
    "0x8180D59b7175d4064bDFA8138A58e9baBFFdA44a": "EmergencyProposalExecutor",
    "0x50efaC9619225d7fB4703C5872da978849B6E7cC": "ProposerV2",
