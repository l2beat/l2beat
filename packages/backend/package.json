{
  "name": "@l2beat/backend",
  "version": "0.1.0",
  "private": true,
  "main": "./build/data.json",
  "types": "./index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:dependencies": "cd ../.. && yarn build:dependencies",
    "clean": "rm -rf build",
    "discover": "./scripts/discoverAndAddHistory.sh",
    "discover:raw": "discovery discover",
    "discover:dev": "cd ../discovery && yarn build && cd - && yarn discover --dev",
    "discover:single": "discovery single-discovery",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "invert": "discovery invert",
    "lint": "eslint --ext .ts,.tsx --max-warnings 0 src",
    "lint:fix": "yarn lint --fix",
    "run-kroma-propose-script": "ts-node ./discovery/kroma/ethereum/script-ts/propose.ts",
    "start": "node -r source-map-support/register build/src/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "mocha",
    "typecheck": "tsc --noEmit",
    "find-common-contracts": "ts-node --transpile-only src/tools/findCommonContracts.ts",
    "update-diff-history": "ts-node --transpile-only src/tools/updateDiffHistory.ts"
  },
  "dependencies": {
    "@bugsnag/js": "^7.22.3",
    "@bugsnag/plugin-koa": "^7.19.0",
    "@google-cloud/bigquery": "^7.3.0",
    "@koa/router": "^12.0.0",
    "@l2beat/backend-tools": "^0.5.0",
    "@l2beat/config": "*",
    "@l2beat/discovery": "0.28.3",
    "@l2beat/discovery-types": "^0.7.0",
    "@l2beat/shared": "*",
    "@l2beat/shared-pure": "*",
    "@l2beat/uif": "^0.2.3",
    "basic-auth": "^2.0.1",
    "chalk": "^4.1.2",
    "deep-diff": "^1.0.2",
    "ethers": "^5.7.2",
    "jsonc-parser": "^3.2.0",
    "knex": "^2.4.2",
    "koa": "^2.14.1",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.0",
    "node-fetch": "^2.6.7",
    "pg": "^8.8.0",
    "prom-client": "^14.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "^5.0.0",
    "source-map-support": "^0.5.21",
    "twitter-api-v2": "^1.14.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/deep-diff": "^1.0.2",
    "@types/koa": "^2.13.5",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-etag": "^3.0.0",
    "@types/koa__router": "^12.0.0",
    "@types/lodash": "^4.14.191",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "8.6.5",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/supertest": "^2.0.12",
    "eslint-plugin-custom-rules": "file:./.eslint",
    "prettier": "^2.8.3",
    "supertest": "^6.2.3",
    "ts-node-dev": "^2.0.0"
  }
}
