  "overrides": {
    "SecurityCouncil": {
      "references": [
        {
          "text": "Security Council members - Scroll Docs",
          "href": "https://scroll.io/gov-docs/content/security-council"
        }
      ]
    },
    "pufETHEscrow": {
      "ignoreRelatives": ["owner", "$admin", "counterpart"]
    },
    "DaiEscrow": {
      "ignoreRelatives": ["counterpart"]
    },
    "wstETHescrowLido": {
      "ignoreMethods": ["l1Token", "l2Token"],
      "ignoreRelatives": ["owner", "$admin"]
    },
    "L1MessageQueue": {
      "ignoreMethods": [
        "estimateCrossDomainMessageFee",
        "getCrossDomainMessage",
        "isMessageDropped",
        "isMessageSkipped",
        "messageQueue"
      ],
      "ignoreInWatchMode": [
        "nextCrossDomainMessageIndex",
        "pendingQueueIndex",
        "l2BaseFee",
        "nextUnfinalizedQueueIndex"
      ]
    },
    "L1ScrollMessenger": {
      "ignoreMethods": ["prevReplayIndex"]
    },
    "OLD_L2GasPriceOracle": {
      "ignoreMethods": ["estimateCrossDomainMessageFee"],
      "ignoreInWatchMode": ["l2BaseFee"]
    },
    "ScrollChain": {
      "ignoreMethods": [
        "committedBatches",
        "finalizedStateRoots",
        "isBatchFinalized",
        "withdrawRoots"
      ],
      "ignoreInWatchMode": ["lastFinalizedBatchIndex"],
      "fields": {
        "sequencers": {
          "handler": {
            "type": "event",
            "select": "account",
            "add": {
              "event": "UpdateSequencer",
              "where": ["=", "#status", true]
            },
            "remove": {
              "event": "UpdateSequencer",
              "where": ["!=", "#status", true]
            }
          }
        },
        "provers": {
          "handler": {
            "type": "event",
            "select": "account",
            "add": {
              "event": "UpdateProver",
              "where": ["=", "#status", true]
            },
            "remove": {
              "event": "UpdateProver",
              "where": ["!=", "#status", true]
            }
          }
        },
        "revertedBatches": {
          "handler": {
            "type": "event",
            "select": "batchIndex",
            "add": { "event": "RevertBatch" }
          }
        }
      }
    },
    "MultipleVersionRollupVerifier": {
      "description": "Used to update the verifier and keep track of current and old versions.",
      "ignoreMethods": ["getVerifier"],
      "fields": {
        "verifierVersions": {
          "handler": {
            "type": "event",
            "select": "version",
            "add": { "event": "UpdateVerifier" }
          }
        },
        "latestVerifier": {
          "handler": {
            "type": "array",
            "maxLength": 20,
            "indices": "{{ verifierVersions }}"
          },
          "returnType": "((uint64 startBatchIndex, address verifier)[])"
        },
        "legacyVerifiersLength": {
          "handler": {
            "type": "array",
            "maxLength": 20,
            "indices": "{{ verifierVersions }}"
          }
        }
      }
    },
    "ScrollOwner": {
      "description": "Owner of all contracts in the system. It implements an extension of AccessControl that manages roles and functions allowed to be called by each role.",
      "fields": {
        "accessControl": {
          "handler": {
            "type": "scrollAccessControl",
            "roleNames": {
              "0xa35737ada2a80e3013b3b2c3a23e5f55c43393a64348742b3518ec0eff3de8e6": "SECURITY_COUNCIL_NO_DELAY_ROLE",
              "0x357f58d6582a9aad552073fb90e586033d651a1117d026182f09ca03868d3e85": "SCROLL_MULTISIG_NO_DELAY_ROLE",
              "0x640b21f88436d06e77efa2750ca42147d57f927008d06e80d75923e2e22cf761": "EMERGENCY_MULTISIG_NO_DELAY_ROLE",
              "0x0bc16a0ae95f4e5b799d8e99b60c3f58a85a250a4113bda01d7aa9161915bb51": "emergency-nodelay",
              "0x25cde7d55aec2382cb151a266953ae458755f20dbd760809260e51683955fe9e": "ops-fast",
              "0x7073af4b167920283f68deceb22f52cc6f5d834eeb5d5ba612c774b2bcfa0d79": "TIMELOCK_1DAY_DELAY_TOLE",
              "0x7101c64e1cfe9c2a867a931776e9ff5db14162149d7281b6372f94dcc3fcef35": "TIMELOCK_7DAY_DELAY_ROLE"
            }
          }
        }
      }
    },
    "TimelockSlow": {
      "fields": {
        "accessControl": {
          "handler": {
            "type": "accessControl"
          }
        }
      }
    },
    "TimelockFast": {
      "fields": {
        "accessControl": {
          "handler": {
            "type": "accessControl"
          }
        }
      }
    },
    "TimelockSC": {
      "fields": {
        "accessControl": {
          "handler": {
            "type": "accessControl"
          }
        }
      }
    },
    "TimelockEmergency": {
      "fields": {
        "accessControl": {
          "handler": {
            "type": "accessControl"
          }
        }
      }
    },
    "L1USDCGateway": {
      "ignoreMethods": ["l1USDC"],
      "ignoreInWatchMode": ["totalBridgedUSDC"]
    },
    "L1WETHGateway": {
      "ignoreMethods": ["WETH"]
    },
    "Whitelist": {
      "fields": {
        "whitelisted": {
          "handler": {
            "type": "event",
            "select": "_account",
            "add": {
              "event": "WhitelistStatusChanged",
              "where": ["=", "#_status", true]
            },
            "remove": {
              "event": "WhitelistStatusChanged",
              "where": ["!=", "#_status", true]
            }
          }
        }
      }
    },
    "EnforcedTxGateway": {
      "fields": {
        "paused": {
          "description": "Whether the sendTransaction function is paused or not. Affects the sequencer failure risk.",
          "severity": "HIGH"
        }
      }
    },
    "PlonkVerifierV0": {
      "manualSourcePaths": {
        "0x4B8Aa8A96078689384DAb49691E9bA51F9d2F9E1": "https://circuit-release.s3.us-west-2.amazonaws.com/release-v0.9.5/evm_verifier.yul"
      }
    },
