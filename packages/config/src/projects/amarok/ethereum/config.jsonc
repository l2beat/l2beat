      "ignoreInWatchMode": ["nonce"]
    },
    "0x2C217D709A9309b1D30323bAcE28438eDe7E4e05": {
      "ignoreDiscovery": true
    },
    "0x1cEB5cB57C4D4E2b2433641b95Dd330A33185A44": {
      "ignoreDiscovery": true
    },
