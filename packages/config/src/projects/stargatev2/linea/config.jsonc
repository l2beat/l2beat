{
  "$schema": "../../../../../discovery/schemas/config.v2.schema.json",
  "chain": "linea",
  "name": "stargatev2",
  "import": ["../../globalConfig.jsonc"],
  "initialAddresses": ["0x5f688F563Dc16590e570f97b542FA87931AF2feD"],
