{
  "name": "@l2beat/token-db",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf build",
    "db:export": "tsx src/scripts/export.ts",
    "dev": "tsx --no-warnings=ExperimentalWarning --watch src/main.ts",
    "db:seed": "tsx src/scripts/seed.ts",
    "format": "biome check .",
    "format:fix": "biome check --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "start": "tsx src/main.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "^5.17.1",
    "@bull-board/express": "^5.17.1",
    "@l2beat/backend-tools": "workspace:*",
    "@l2beat/database": "workspace:*",
    "@l2beat/shared-pure": "workspace:*",
    "@types/express": "^4.17.21",
    "bullmq": "^5.7.8",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.7",
    "papaparse": "^5.4.1",
    "parse-redis-url-simple": "^1.0.2",
    "viem": "^2.10.1",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@l2beat/biome-config": "workspace:*",
    "@l2beat/typescript-config": "workspace:*",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.10",
    "@types/papaparse": "^5.3.14",
    "tsx": "^4.9.3",
    "type-fest": "^4.18.2"
  }
}
