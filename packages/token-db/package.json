{
  "name": "@l2beat/token-db",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:1-generate": "prisma generate",
    "build:2-compile": "tsc",
    "clean": "rm -rf build",
    "db:export": "tsx src/scripts/export.ts",
    "dev": "prisma generate > /dev/null && tsx --no-warnings=ExperimentalWarning --watch src/main.ts",
    "db:format": "prisma format",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "prisma studio",
    "format": "biome check .",
    "format:fix": "biome check --apply .",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --apply",
    "nanoid": "nanoid",
    "seed": "tsx src/db/seed.ts",
    "start": "tsx src/main.ts",
    "start:queue": "tsx --no-warnings=ExperimentalWarning --watch src/queue.ts",
    "typecheck": "prisma generate && tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "^5.17.1",
    "@bull-board/express": "^5.17.1",
    "@l2beat/backend-tools": "*",
    "@l2beat/shared-pure": "*",
    "@prisma/client": "5.19.1",
    "@types/express": "^4.17.21",
    "bullmq": "^5.7.8",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.7",
    "papaparse": "^5.4.1",
    "parse-redis-url-simple": "^1.0.2",
    "postgres": "^3.4.4",
    "viem": "^2.10.1",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.10",
    "@types/papaparse": "^5.3.14",
    "npm-run-all": "^4.1.5",
    "prisma": "5.19.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.9.3",
    "type-fest": "^4.18.2"
  }
}
