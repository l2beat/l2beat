{
  "name": "@l2beat/uops-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm prebuild&& next dev --turbopack",
    "prebuild": "pnpm build:config && node ./scripts/getChains.mjs",
    "build": "next build",
    "start": "next start",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "lint:fix": "biome check --formatter-enabled=false --write .",
    "lint": "biome check --formatter-enabled=false .",
    "typecheck": "if [ ! -e ./src/chains.json ]; then echo \"[]\" > ./src/chains.json; fi && tsc --noEmit",
    "test": "mocha",
    "build:config": "cd ../config && pnpm build"
  },
  "dependencies": {
    "@l2beat/validate": "workspace:*",
    "@l2beat/backend-tools": "workspace:*",
    "@l2beat/shared": "workspace:*",
    "@l2beat/shared-pure": "workspace:*",
    "ethers": "^5.7.2",
    "flowbite-react": "^0.10.1",
    "next": "15.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@l2beat/typescript-config": "workspace:*",
    "@l2beat/config": "workspace:*",
    "@types/node": "^20",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/uuid": "^10.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  },
  "overrides": {
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4"
  }
}
