{
  "name": "@l2beat/dal",
  "version": "0.1.0",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json && pnpm run packageHash",
    "packageHash": "node -r esbuild-register scripts/packageHash/index.ts",
    "clean": "rm -rf build",
    "format:fix": "biome format --write .",
    "format": "biome format .",
    "lint:fix": "biome check --formatter-enabled=false --write .",
    "lint": "biome check --formatter-enabled=false .",
    "test": "mocha",
    "typecheck": "tsc --noEmit",
    "build:dependencies": "cd ../.. && pnpm build:dependencies:dal",
    "watch:dependencies": "cd ../.. && pnpm watch:dependencies:dal"
  },
  "dependencies": {
    "@l2beat/backend-tools": "workspace:*",
    "@l2beat/config": "workspace:*",
    "@l2beat/database": "workspace:*",
    "@l2beat/shared-pure": "workspace:*",
    "@l2beat/typescript-config": "workspace:*",
    "@l2beat/validate": "workspace:*",
    "@types/redis": "^4.0.11",
    "redis": "^4.7.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@l2beat/typescript-config": "workspace:*",
    "@types/mocha": "^10.0.6",
    "earl": "^1.1.0",
    "mocha": "^10.2.0",
    "@types/lodash": "^4.17.4"
  }
}
