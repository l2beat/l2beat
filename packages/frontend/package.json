{
  "name": "@l2beat/frontend",
  "version": "0.1.0",
  "private": true,
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0 <2"
  },
  "scripts": {
    "build": "NODE_ENV=production gulp build",
    "build:metaimages": "NODE_ENV=production GENERATE_METAIMAGES=true gulp build",
    "build:dependencies": "cd ../.. && yarn build:dependencies",
    "clean": "rm -rf build",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx --max-warnings 0 src",
    "start": "yarn start:staging",
    "start:local": "DEPLOYMENT_ENV=local gulp watch",
    "start:mock": "DEPLOYMENT_ENV=local MOCK=true gulp watch",
    "start:staging": "DEPLOYMENT_ENV=staging gulp watch",
    "start:production": "DEPLOYMENT_ENV=production gulp watch",
    "test": "mocha",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "(cd ../.. && wsrun -p @l2beat/{shared-pure,config} -recsm build --module es6) && storybook build"
  },
  "dependencies": {
    "@bugsnag/js": "^7.22.3",
    "@l2beat/config": "*",
    "@l2beat/shared-pure": "*",
    "@types/express": "^4.17.16",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/markdown-it": "^12.2.3",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "clsx": "^2.1.0",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "puppeteer": "^14.3.0",
    "qrcode": "^1.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "7.0.9",
    "@storybook/addon-essentials": "7.0.9",
    "@storybook/addon-interactions": "7.0.9",
    "@storybook/addon-styling": "^1.0.6",
    "@storybook/addons": "7.0.9",
    "@storybook/react": "7.0.9",
    "@storybook/react-vite": "7.0.9",
    "@storybook/testing-library": "^0.2.2",
    "autoprefixer": "^10.4.13",
    "chromatic": "^7.4.0",
    "del": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.15",
    "gulp": "^4.0.2",
    "http-proxy-middleware": "^2.0.6",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "storybook": "7.0.9",
    "storybook-addon-pseudo-states": "^2.1.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.1",
    "vite": "^3.0.9",
    "vite-plugin-turbosnap": "^1.0.3"
  }
}
