{
  "name": "@l2beat/frontend",
  "version": "0.1.0",
  "private": true,
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production gulp build",
    "prebuild": "yarn clean",
    "clean": "rm -rf build",
    "format:fix": "prettier --write .",
    "format": "prettier --check .",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx --max-warnings 0 src",
    "start": "yarn start:staging",
    "start:local": "DEPLOYMENT_ENV=local gulp watch",
    "start:staging": "DEPLOYMENT_ENV=staging gulp watch",
    "start:production": "gulp watch",
    "test": "mocha",
    "typecheck:ci": "yarn typecheck",
    "typecheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6006 --no-open",
    "storybook:build": "(cd ../.. && wsrun -p @l2beat/{shared,config} -recsm build --module es6) && build-storybook",
    "storybook:screenshot": "yarn storycap --serverCmd \"npx http-server ./storybook-static -p 9001\" http://localhost:9001 -V 2560x1440 -V 390x844"
  },
  "dependencies": {
    "@l2beat/config": "*",
    "@l2beat/shared": "*",
    "@types/express": "^4.17.16",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/markdown-it": "^12.2.3",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "cheerio": "^1.0.0-rc.11",
    "classnames": "^2.3.2",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "puppeteer": "^14.3.0",
    "qrcode": "^1.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.10",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.5",
    "cssnano": "^5.1.14",
    "del": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.0.0",
    "http-proxy-middleware": "^2.0.6",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "sass": "^1.52.2",
    "storybook-dark-mode": "^1.1.0",
    "storycap": "^3.1.9",
    "tailwindcss": "^3.1.0",
    "vite": "^3.0.9"
  }
}
