{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build", "db:generate-types"],
      "env": [
        "DATABASE_URL",
        "EXCLUDED_TVL_PROJECTS",
        "EXCLUDED_ACTIVITY_PROJECTS",
        "ETHEREUM_RPC_URL",
        "FEATURE_FLAG_GITCOIN_OPTION",
        "FEATURE_FLAG_HIRING",
        "MOCK",
        "NODE_ENV",
        "PORT",
        "SKIP_ENV_VALIDATION",
        "VERCEL_GIT_COMMIT_REF",
        "VERCEL_GIT_COMMIT_SHA",
        "VERCEL_ENV",
        "VERCEL_URL"
      ],
      "outputs": [".next/**", "!.next/cache/**"]
    }
  }
}
