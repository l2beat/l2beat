{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "target": "ESNext",
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "noUncheckedIndexedAccess": true,
    "module": "ESNext",
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "react": ["./node_modules/@types/react"]
    }
  }
}
