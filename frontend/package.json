{
  "name": "l2-beat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn dev",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build && yarn gen:og-img && next build",
    "format": "prettier --check \"./**/*.{ts,tsx,scss}\"",
    "format:fix": "prettier --write \"./**/*.{ts,tsx,scss}\"",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "test:fix": "yarn lint:fix && yarn format:fix && yarn typecheck",
    "typecheck": "tsc --noEmit",
    "gen:og-img": "next export && ts-node --project ./tsconfig.json ./scripts/generateOgImages.ts"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/lodash": "^4.14.168",
    "classnames": "^2.2.6",
    "eslint": "^6.8.0",
    "eslint-config-typestrict": "^1.0.0",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "http-server": "^0.12.3",
    "human-readable-numbers": "^0.9.5",
    "lodash": "^4.17.21",
    "millify": "^3.5.0",
    "next": "^10.1.1",
    "node-sass": "^5.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-vis": "^1.11.7",
    "ts-essentials": "^7.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/node": "^14.14.33",
    "@types/react": "^17.0.3",
    "@types/react-vis": "^1.11.6",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "capture-website-cli": "^1.1.0",
    "prettier": "^2.2.1"
  }
}
